{
  "devDependencies": {
    "gh-pages": "^6.3.0"
  },
  "scripts": {
    "build:testimonial-card": "cd testimonial-card && npm install && npm run build",
    "deploy:testimonial-card": "gh-pages -d testimonial-card/dist --dest testimonial-card --src '**/*' -m 'Deploy testimonial-card'",
    "build:blog-card": "cd blog-card && npm install && npm run build",
    "deploy:blog-card": "gh-pages -d blog-card/dist --dest blog-card --src '**/*' -m 'Deploy blog-card'",
    "deploy:profile-card": "gh-pages -d profile-card --dest profile-card --src '**/*' -m 'Deploy profile-card'"
  }
}